{
  "name": "project-dynamo",
  "version": "0.5.1",
  "description": "pixi.js typescript template",
  "scripts": {
    "test": "npx jest",
    "code-coverage": "jest --coverage",
    "dev": "parcel ./src/index.html --port=8080",
    "dev:open": "parcel ./src/index.html --port=8080 --open",
    "prebuild": "npx del-cli ./dist",
    "build": "parcel build ./src/index.html --public-url ./dist/"
  },
  "author": "bySabi Files",
  "license": "MIT",
  "dependencies": {
    "@pixi/sound": "^4.0.4",
    "alea": "^1.0.0",
    "howler": "^2.2.3",
    "intersects": "^2.7.2",
    "keycon": "^1.1.2",
    "pixi.js": "^5.3.0"
  },
  "devDependencies": {
    "@timohausmann/quadtree-js": "^1.2.4",
    "@types/intersects": "^2.5.0",
    "@types/jest": "^26.0.0",
    "@types/jsdom": "^16.2.3",
    "del-cli": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-canvas-mock": "^2.2.0",
    "jsdom": "^16.2.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "ts-jest": "^26.1.1",
    "typescript": "^4.2.4"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "./src/assets",
        "staticOutDir": "./assets"
      }
    ],
    "watcherGlob": "**"
  }
}
